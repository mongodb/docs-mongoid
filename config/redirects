define: prefix docs/mongoid
define: base https://www.mongodb.com/${prefix}
define: versions v9.0 master

symlink: current -> v9.0
symlink: upcoming -> master

raw: ${prefix}/ -> ${base}/current/
raw: ${prefix}/stable -> ${base}/current/

# Redirects from old/legacy docs to standardized docs
[*-v9.0]: ${prefix}/${version}/installation/ -> ${base}/upcoming/#quick-start
[*-v9.0]: ${prefix}/${version}/reference/compatibility/ -> ${base}/upcoming/compatibility/










